    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [928, 128.0]
    rotation: 0
    state: true

connections:
- [analog_noise_source_x_0, '0', blocks_add_xx_0, '0']
- [analog_sig_source_x_0, '0', blocks_add_xx_0, '1']
- [blocks_add_xx_0, '0', pipe_filter_0, '0']
- [blocks_throttle_0, '0', qtgui_sink_x_0, '0']
